  "services": [
    {
      "id": "minio_grpc_service",
      "name": "minio_grpc_service",
      "port": 50051,
      "address": "isa-minio-grpc",
      "tags": ["v1", "grpc", "infrastructure", "staging", "storage"],
      "meta": {
        "version": "1.0.0",
        "environment": "staging",
        "team": "infrastructure",
        "protocol": "grpc"
      },
      "checks": [
        {
          "id": "minio-grpc-tcp-check",
          "name": "MinIO gRPC Service TCP Check",
          "tcp": "isa-minio-grpc:50051",
          "interval": "10s",
          "timeout": "3s",
          "deregister_critical_service_after": "90s"
        }
      ]
    },
